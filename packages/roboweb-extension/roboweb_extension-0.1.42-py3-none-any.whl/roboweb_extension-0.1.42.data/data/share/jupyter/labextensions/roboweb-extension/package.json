{
  "name": "roboweb-extension",
  "version": "0.1.0",
  "description": "Roboweb extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/github_username/roboweb-extension",
  "bugs": {
    "url": "https://github.com/github_username/roboweb-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/github_username/roboweb-extension.git"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Francisco Uribe",
    "email": "francisco.uribe@gmail.com"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "scripts": {
    "build": "npm run build:labextension:dev",
    "build:all": "npm run build",
    "build:labextension": "python3 -m jupyter labextension build .",
    "build:labextension:dev": "python3 -m jupyter labextension build --development True .",
    "build:prod": "npm run build:labextension",
    "clean:all": "npm run clean:labextension",
    "clean:labextension": "rimraf roboweb_extension/labextension",
    "prepare": "npm run build:prod",
    "test": "jest",
    "watch": "npm run watch:labextension",
    "watch:labextension": "python3 -m jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^3.6.2"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.6.2",
    "jest": "^29.5.0",
    "jsdom": "^21.1.2",
    "rimraf": "~3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "roboweb_extension/labextension",
    "_build": {
      "load": "static/remoteEntry.52495129d51f16d70f5d.js",
      "extension": "./extension"
    }
  }
}
